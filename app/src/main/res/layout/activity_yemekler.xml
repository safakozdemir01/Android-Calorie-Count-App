        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.487" />

    <EditText
        android:id="@+id/txtMiktar"
        android:layout_width="202dp"
        android:layout_height="43dp"
